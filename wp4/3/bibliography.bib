@misc{tsengdar_lee_challenging_2013,
  title = {Challenging {{Large}}-{{Data Problems}} at {{NASA}}},
  abstract = {In this video from the 2013 National HPCC Conference, Dr. Tsengdar Lee from NASA presents: Challenging Large-Data Problems at NASA.

"NASA's observations from Earth-orbiting satellites and outputs from computational climate models have contributed to one of the most data-intensive scientific disciplines today. The Earth system science tries to analyze the data, turns the data into information, makes sense of the information into knowledge and wisdom, utilize the knowledge and wisdom in decision making processes. In every step of the data life cycle workflow (i.e. curation, discovery, access, and analysis), NASA faces tremendous challenges.

 Many times we like to draw parallels the library science and the data science in our distributed environment. NASA Distributed Active Archive Centers (DAACs) are the curator and steward of the data. A tradition workflow focuses on indexing the data for easy search and disseminate the data through a FTP or web server. This is very similar to a user searching out a publication and checking out a book at a library. Increasingly, this traditional workflow is no longer working in this digital age. When the data volume becomes large, interdisciplinary science becomes prevalent, multi-dimensional data analysis becomes necessary, our existing data management infrastructure and workflows have become inadequate. NASA not only has to provide archival, distribution, and stewardship of the data, but we also have to develop and share tools that simplify access and utilization of data, as well as address the large computational demands associated with the data utilization. New challenges have surfaced every day.

In this talk, I will give examples of these challenges and discuss the approach we have taken in meeting these challenges.  I will also discuss how the HPC community may help in facing some of the technical challenges with the hope to expand the business case for the HPC community."},
  timestamp = {2016-09-01T14:12:31Z},
  urldate = {2016-09-01},
  url = {https://www.youtube.com/watch?v=F_jWSAvM7mI},
  author = {{Tsengdar Lee}},
  month = mar,
  year = {2013},
  keywords = {big data,climate,hpc,insideHPC,NASA,National HPCC Conference,supercomputing}
}

