  month = oct,
  year = {2016},
  keywords = {Computer Science - Information Theory,Computer Science - Networking and Internet Architecture},
  file = {Siavoshani et al_2016_On Communication Cost vs.pdf:/home/pq/.mozilla/firefox/7yatqkvb.default/zotero/storage/RD9ZWTF2/Siavoshani et al_2016_On Communication Cost vs.pdf:application/pdf;arXiv.org Snapshot:/home/pq/.mozilla/firefox/7yatqkvb.default/zotero/storage/HMAS5S8S/1610.html:text/html}
