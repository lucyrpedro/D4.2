@article{CECOSDKMKL10,
	author	 = {Julian Kunkel and Olga Mordvinova and Michael Kuhn and Thomas Ludwig},
	title	 = {{Collecting Energy Consumption of Scientific Data}},
	year	 = {2010},
	editor	 = {Thomas Ludwig},
